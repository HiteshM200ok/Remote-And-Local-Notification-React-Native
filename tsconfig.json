{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths":{
      "@assets/*":[
        "source/Assets/*"
      ],
      "@navigation/*":[
        "source/navigation/*"
      ],
      "@context/*":[
        "source/Context/*"
      ],
      "@notifications":[
        "source/context/Notifications"
      ],
      "@notifiation-root":[
        "source/Context/Notifications/NotificationsContextRoot"
      ],
      "@screens/*":[
        "source/Screens/*"
      ],
      "@services/*":[
        "source/Services/*"
      ],
      "@app/*":[
        "source/App/*"
      ],
      "@component-root":[
        "source/Components/ComponentRoot"
      ],
    }
  }
}
